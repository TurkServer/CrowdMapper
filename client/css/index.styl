/* CSS declarations go here */

body
    line-height: 1
    padding: 20px 50px
    height: 100%

ol, ul
    list-style: none;

blockquote, q
    quotes: none;

blockquote:before, blockquote:after, q:before, q:after
    content: '';
    content: none;

table
    border-collapse: collapse;
    border-spacing: 0;

div.centered
    width: 100%
    max-width: 1280px
    margin-left: auto
    margin-right: auto
